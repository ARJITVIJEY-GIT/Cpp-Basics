#include <iostream>

using namespace std;

class stu
{
private:
    int id;
    char name[20];
    float fee;
public:
    void get()
    {
        cout<<" Enter id,name,fee : ";
        cin>>id>>name>>fee;
        cout<<"id ="<<id<<endl;
        cout<<"name ="<<name<<endl;
        cout<<"fee = "<<fee;
    }
};
int main()
{
    stu s;
    s.get();
}
