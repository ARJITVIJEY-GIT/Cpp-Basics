#include<iostream>
using namespace std;
int main()
 {
int year;
cout<<"Enter the year you want to check\n";
cin>>year;
if(year%400==0)
{
    cout<<"It is a leap year\n",year;
}
else if(year%100==0)
{
    cout<<"It is not a leap year\n",year;
}
else if(year%4==0)
{
    cout<<"It is a leap year\n",year;
}
else
{
    cout<<"It is not a leap year\n",year;
}
}

